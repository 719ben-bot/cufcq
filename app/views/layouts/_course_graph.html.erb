<% @course.overall_query  %>
<script type="text/javascript">
  $(function () {
    new Highcharts.Chart({
      title: {text: null
          },
      chart: { renderTo: 'container' },
        legend: {
            enabled: true
        },
        xAxis: {
            tickPositions: [],
            type: 'category',
            categories: []
        },
        yAxis: {
          max: 6,
          min: 0,
          title: {
            text: null
            }
        },
      tooltip: {
        formatter: function () {
          var n = this.x%10
          var y = Math.floor(this.x/10);
          var s = n == 1 ? "Spring " : n==4 ? "Summer " : n==7 ? "Fall " : "Error "
          var parsed = s+y+" : "+this.y
          return (this.y >= 1.0) ? parsed : parsed+"<br/>NOTE: during this semester, the instructor(s) of <br/>these course(s) did not return any FCQs. As a result, <br/>this value is marked as 0.0. Student's cannot <br/>actually mark scores lower than 1.0. This <br/>value is meant to serve as an indicator <br/>of an error."
          return s+y+" : "+this.y
      }
    },
    exporting: {
        buttons: {
            contextButtons: {
                enabled: false,
                menuItems: null
            }
        },
        enabled: true
    },    
    series: [
    {
        data: <%= @course.overall_data %>,
        name: "Overall"
    },
    {
        data: <%= @course.challenge_data %>,
        name: "Challenge"
    },
    {
        data: <%= @course.interest_data %>,
        name: "Prior Interest"
    },
    {
        data: <%= @course.learned_data %>,
        name: "Amount Learned"
    }
    ]
    });
  });
</script>
<div id="container" style="height: 300px"></div>