     <div class="container-fluid">
          <div class="row">
            <div class="col-sm-3 col-md-2 sidebar">
              <ul class="nav nav-sidebar">
                <li><a href="/departments">Departments<span class="sr-only">(current)</span></a></li>
                <li><a href="/courses">Courses<span class="sr-only">(current)</span></a></li>
              </ul>

      </div>

    <title>FCQs</title>
    <div class="container-fluid">
      <div class="row">
        <div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">

          <h2 class="sub-header">Instructors</h2>
          <div class="table-responsive">
            <table class="table table-striped">

              <thead>
                <tr>
                  <th>Corrected Title</th>
                  <th>Yearterm</th>
                  <th>Subject</th>
                  <th>Crse</th>
                  <th>Sec</th>
                  <th>Instructor last</th>
                  <th>Instructor first</th>
                  <th>Forms requested</th>
                  <th>Forms returned</th>
                  <th>Percentage passed</th>
                  <th>Course overall</th>
                  <th>Course overall sd</th>
                  <th>Instructor overall</th>
                  <th>Instructor overall sd</th>
                  <th>Total hours</th>
                  <th>Prior interest</th>
                  <th>Effectiveness</th>
                  <th>Availability</th>
                  <th>Challenge</th>
                  <th>Amount learned</th>
                  <th>Respect</th>
                  <th>Course title</th>
                  <th>Campus</th>
                  <th>College</th>
                  <th>Instructor group</th>
                  <th>is recitation?</th>
                </tr>
              </thead>
              
              <tbody>
               <% @fcqs.each do |fcq| %>
                    <tr>
                      <td><%= fcq.yearterm %></td>
                      <td><%= fcq.subject %></td>
                      <td><%= fcq.crse %></td>
                      <td><%= fcq.sec %></td>
                      <td><%= fcq.instructor_last %></td>
                      <td><%= fcq.instructor_first %></td>
                      <td><%= fcq.formsrequested %></td>
                      <td><%= fcq.formsreturned %></td>
                      <td><%= fcq.percentage_passed %></td>
                      <td><%= fcq.courseoverall %></td>
                      <td><%= fcq.courseoverall_SD %></td>
                      <td><%= fcq.instructoroverall %></td>
                      <td><%= fcq.instructoroverall_SD %></td>
                      <td><%= fcq.hoursperwkinclclass %></td>
                      <td><%= fcq.priorinterest %></td>
                      <td><%= fcq.instreffective %></td>
                      <td><%= fcq.availability %></td>
                      <td><%= fcq.challenge %></td>
                      <td><%= fcq.howmuchlearned %></td>
                      <td><%= fcq.instrrespect %></td>
                      <td><%= fcq.course_title %></td>
                      <td><%= fcq.campus %></td>
                      <td><%= fcq.college %></td>
                      <td><%= fcq.instr_group %></td>
                      <td><%= fcq.recitation? %></td>
                      <td><%= fcq.corrected_course_title %></td>
                    </tr>
                  <% end %>
            </table>
          </div>
        </div>
      </div>
    </div>




